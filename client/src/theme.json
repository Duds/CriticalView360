{
  "palette": {
    "mode": "light",
    "primary": {
      "main": "#006496",
      "light": "#1F98DF",
      "dark": "#004B72",
      "contrastText": "#FFFFFF"
    },
    "secondary": {
      "main": "#50606F",
      "light": "#8392A3",
      "dark": "#394856",
      "contrastText": "#FFFFFF"
    },
    "error": {
      "main": "#BA1A1A",
      "light": "#DE3730",
      "dark": "#93000A",
      "contrastText": "#FFFFFF"
    },
    "text": {
      "primary": "rgba(0, 0, 0, 0.87)",
      "secondary": "#50606F"
    },
    "background": {
      "default": "#FCFCFF"
    },
    "action": {
      "active": "#008FD5",
      "hover": "#006496",
      "selected": "#CCE5FF"
    }
  },
  "typography": {
    "fontFamily": "Inter, sans-serif",
    "h1": {
      "fontWeight": 800
    },
    "h2": {
      "fontWeight": 800
    },
    "h3": {
      "fontWeight": 800
    },
    "h4": {
      "fontWeight": 600
    },
    "h5": {
      "fontWeight": 500
    },
    "h6": {
      "fontWeight": 400
    },
    "subtitle1": {
      "fontSize": 20,
      "fontWeight": 400,
      "lineHeight": "1.5",
      "letterSpacing": "0.5px",
      "marginBottom": "24px"
    },
    "body1": {
      "fontSize": 16,
      "fontWeight": 400,
      "lineHeight": "1.5",
      "letterSpacing": "0.5px",
      "marginBottom": "24px"
    },
    "clampText": {
      "fontSize": "clamp(24px, 3.33vw, 57px)"
      }
  },
  "shape": {
    "borderRadius": 4
  },
  "components": {
    "MuiButton": {
      "styleOverrides": {
        "root": {
          "textTransform": "none"
        },
        "containedPrimary": {
          "backgroundColor": "#008FD5"
        },
        "outlinedPrimary": {
          "color": "#008FD5",
          "borderColor": "#008FD5"
        }
      }
    },
    "MuiAppBar": {
      "styleOverrides": {
        "root": {
          "&.MuiAppBar-colorPrimary": {
            "backgroundColor": "transparent",
            "color": "text.primary",
            "boxShadow": "none"
          }
        }
      }
    }
  }
}